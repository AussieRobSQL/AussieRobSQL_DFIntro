					},
					"condition": {
						"value": "@equals(item(), 'value3')",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"inputArray": {
				"type": "Array"
			}
		},
		"folder": {
			"name": "Demo 3"
		}
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}